		<cpu:CopyImmediateToDX/>
		<hex>000003da</hex>

		<label id="waitForVSyncOff"/>
		
		<cpu:ReadFromDXPortToAL/>

		<cpu:TestALWithImmediate/>
		<binary>00001000</binary>

		<cpu:LoopToRelative8WhileNotZero/>
