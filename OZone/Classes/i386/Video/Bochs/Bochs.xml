		<hex>e0000000</hex>

		<!--Get Screen Resolution-->
		<cpu:CopyImmediateToDX/>
		<hex>000001ce</hex>

		<cpu:CopyImmediateToAX/>
		<int>1</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<cpu:IncrementDX/>

		<cpu:Operand16/>
		<cpu:ReadFromDXPortToAX/>

		<cpu:CopyRegisterToOperand/>
		<op:AX-BXRegister/>

		<!--Get Point-->
