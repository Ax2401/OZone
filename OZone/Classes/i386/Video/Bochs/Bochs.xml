	</cls:method>

	<cls:method name="Test" type="http://metalx.org/Hexadecimal">
		<!--Disable Video-->
		<cpu:CopyImmediateToDX/>
		<hex>000001ce</hex>

		<cpu:CopyImmediateToAX/>
		<int>4</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<cpu:IncrementDX/>

		<cpu:CopyImmediateToAX/>
		<int>0</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<!--Set Color Depth-->
		<cpu:DecrementDX/>

		<cpu:CopyImmediateToAX/>
		<int>3</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<cpu:IncrementDX/>

		<cpu:CopyImmediateToAX/>
		<int>32</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<!--Set Screen Height-->
		<cpu:DecrementDX/>

		<cpu:CopyImmediateToAX/>
		<int>2</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<cpu:IncrementDX/>

		<cpu:CopyImmediateToAX/>
		<int>768</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<!--Set Screen Width-->
		<cpu:DecrementDX/>

		<cpu:CopyImmediateToAX/>
		<int>1</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<cpu:IncrementDX/>

		<cpu:CopyImmediateToAX/>
		<int>1024</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<!--Enable Video (Linear Buffer)-->
		<cpu:DecrementDX/>

		<cpu:CopyImmediateToAX/>
		<int>4</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<cpu:IncrementDX/>

		<cpu:CopyImmediateToAX/>
		<int>1</int>

		<cpu:OrAXWithImmediate/>
		<hex>00000040</hex>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<cpu:PushDIToStack/>

		<cpu:CopyImmediateToAX/>
		<hex>ffffffff</hex>

		<label id="nextFrame"/>

		<cpu:CopyImmediateToDI/>
		<hex>e0000000</hex>

		<cpu:CopyImmediateToCX/>
		<int>786432</int>

		<cpu:Repeat/>
		<cpu:CopyAXToDIAddressAndIncrementDI/>

		<!--<cpu:ExclusiveOrAXWithImmediate/>
		<hex>ffffffff</hex>-->

		<cpu:IncrementAX/>

		<cpu:JumpToRelative8/>
		<addressOf ref="nextFrame" type="Relative8"/>

		<cpu:PullDIFromStack/>

		<cpu:ReturnToNearCaller/>
	</cls:method>

	<cls:method name="TestMmx" type="http://metalx.org/Hexadecimal">
		<!--Disable Video-->
		<cpu:CopyImmediateToDX/>
		<hex>000001ce</hex>

		<cpu:CopyImmediateToAX/>
		<int>4</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<cpu:IncrementDX/>

		<cpu:CopyImmediateToAX/>
		<int>0</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<!--Set Color Depth-->
		<cpu:DecrementDX/>

		<cpu:CopyImmediateToAX/>
		<int>3</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<cpu:IncrementDX/>

		<cpu:CopyImmediateToAX/>
		<int>32</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<!--Set Screen Height-->
		<cpu:DecrementDX/>

		<cpu:CopyImmediateToAX/>
		<int>2</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<cpu:IncrementDX/>

		<cpu:CopyImmediateToAX/>
		<int>768</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<!--Set Screen Width-->
		<cpu:DecrementDX/>

		<cpu:CopyImmediateToAX/>
		<int>1</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<cpu:IncrementDX/>

		<cpu:CopyImmediateToAX/>
		<int>1024</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<!--Enable Video (Linear Buffer)-->
		<cpu:DecrementDX/>

		<cpu:CopyImmediateToAX/>
		<int>4</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<cpu:IncrementDX/>

		<cpu:CopyImmediateToAX/>
		<int>1</int>

		<cpu:OrAXWithImmediate/>
		<hex>00000040</hex>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<cpu:PushDIToStack/>

		<cpu:CopyImmediateToAX/>
		<hex>ffffffff</hex>

		<label id="nextFrame"/>

		<cpu:CopyImmediateToDI/>
		<hex>e0000000</hex>

		<cpu:CopyImmediateToCX/>
		<int>393216</int>
		
		<cpu:CopyAXToAbsoluteMemory/>
		<addressOf ref="color" type="Absolute32"/>
		
		<cpu:CopyAXToAbsoluteMemory/>
		<addressOf ref="color" type="Absolute32" offset="4"/>
		
		<cpu:ExtendedOperator/>
		<ext:CopyOperandToMmxRegister64 xmlns:ext="http://metalx.org/Intel/80386/ExtendedOperators"/>
		<mmx:MM0-ImmediateAddress xmlns:mmx="http://metalx.org/Intel/80386/MmxOperands"/>
		<addressOf ref="color" type="Absolute32"/>
		
		<label id="nextPixel"/>
		
		<cpu:ExtendedOperator/>
		<ext:CopyMmxRegisterToOperand64 xmlns:ext="http://metalx.org/Intel/80386/ExtendedOperators"/>
		<mmx:MM0-DIAddress xmlns:mmx="http://metalx.org/Intel/80386/MmxOperands"/>
		
		<cpu:MathImmediate8ToOperandFunction/>
		<math:AddToDIRegister/>
		<byte>8</byte>

		<cpu:LoopToRelative8/>
		<addressOf ref="nextPixel" type="Relative8"/>

		<!--<cpu:Repeat/>
		<cpu:CopyAXToDIAddressAndIncrementDI/>-->

		<!--<cpu:ExclusiveOrAXWithImmediate/>
		<hex>ffffffff</hex>-->

		<cpu:IncrementAX/>

		<cpu:JumpToRelative8/>
		<addressOf ref="nextFrame" type="Relative8"/>

		<cpu:PullDIFromStack/>
		
		<label id="color"/>
		<hex>ffffffffffffffff</hex>

		<cpu:ReturnToNearCaller/>
	</cls:method>

