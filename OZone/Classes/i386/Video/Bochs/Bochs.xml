		<cpu:Operand16/>
		<cpu:ReadFromDXPortToAX/>

		<cpu:CopyRegisterToOperand/>
		<op:AX-DIAddress/>

		<cpu:DecrementDX/>

		<cpu:CopyImmediateToAX/>
		<int>2</int>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<cpu:IncrementDX/>

