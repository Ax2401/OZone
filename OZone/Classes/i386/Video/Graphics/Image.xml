		<cpu:LoopToRelative8/>
		<addressOf ref="forEachPixel" type="Relative8"/>

		<cpu:PullSIFromStack/>
		<cpu:PullDIFromStack/>
		<cpu:PullCXFromStack/>
		<cpu:PullBXFromStack/>

		<cpu:AddOperandToRegister/>
		<op:SI-IndexAddressPlusImmediate8/>
		<index:SP/>
		<hex>18</hex>

		<cpu:AddOperandToRegister/>
		<op:DI-IndexAddressPlusImmediate8/>
		<index:SP/>
		<hex>28</hex>

		<cpu:DecrementBX/>

		<cpu:BranchToRelative8IfNotZero/>
		<addressOf ref="forEachRow" type="Relative8"/>

		<label id="skipDraw"/>

		<cpu:MathImmediate8ToOperandFunction/>
		<math:AddToBPRegister/>
		<byte>12</byte>

		<cpu:MathImmediate8ToOperandFunction/>
		<math:AddToSPRegister/>
		<hex>70</hex>

		<cpu:PullDIFromStack/>

		<cls:return/>

		<label id="multiply"/>
		
		<cpu:PushAXToStack/>

		<!--Alpha-->
		<cpu:CopyOperandToRegister8/>
		<op:AL-IndexAddressPlusImmediate8/>
		<index:SP/>
		<byte>3</byte>

