		<cpu:ArithmeticDXAXOperandFunction/>
		<ari:MultiplyByIndexAddressPlusImmediate8/>
		<index:SP/>
		<hex>20</hex>
		
		<cpu:AddOperandToRegister/>
		<op:AX-IndexAddressPlusImmediate8/>
		<index:SP/>
		<hex>60</hex>

		<cpu:ShiftRegisterByImmediate8Function/>
		<shift:ShiftAXRegisterLeft/>
		<byte>2</byte>

		<cpu:AddOperandToRegister/>
		<op:AX-IndexAddressPlusImmediate8/>
		<index:SP/>
		<hex>2c</hex>

		<cpu:CopyRegisterToOperand/>
		<op:AX-DIRegister/>

		<label id="forEachRow"/>

		<cpu:PushCXToStack/>
		<cpu:PushSIToStack/>
		<cpu:PushDIToStack/>

		<cpu:Repeat/>
		<cpu:CopySIAddressToDIAddressAndIncrementSIAndDI/>

		<cpu:PullDIFromStack/>
		<cpu:PullSIFromStack/>
		<cpu:PullCXFromStack/>

		<cpu:AddOperandToRegister/>
		<op:SI-IndexAddressPlusImmediate8/>
		<index:SP/>
		<hex>18</hex>

		<cpu:AddOperandToRegister/>
		<op:DI-IndexAddressPlusImmediate8/>
		<index:SP/>
		<hex>28</hex>

		<cpu:DecrementBX/>

		<cpu:BranchToRelative8IfNotZero/>
