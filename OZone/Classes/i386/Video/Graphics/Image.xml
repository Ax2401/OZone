		<op:AL-IndexAddressPlusImmediate8/>
		<index:SP/>
		<byte>0</byte>

		<cpu:AndAXWithImmediate/>
		<hex>000000ff</hex>

		<cpu:CopyRegisterToOperand/>
		<op:AX-BXRegister/>

