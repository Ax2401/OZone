	<cls:method name="GetEhciControllers" type="http://metalx.org/Usb/Ehci/EhciUsbControllerList" static="true">
		<cpu:CopyImmediateToSI/>
		<int>0</int>

		<cpu:CopyImmediateToCX/>
		<hex>00001000</hex>

		<cpu:CopyImmediateToBX/>
		<int>0</int>

		<label id="forEachSlot"/>

		<cpu:CopyRegisterToOperand/>
		<op:SI-AXRegister/>

		<pcif:GetClassCode/>

		<cpu:AndAXWithImmediate/>
		<hex>ffffff00</hex>

		<cpu:CompareAXToImmediate/>
		<hex>00</hex>
		<hex>20</hex>
		<pcic:UniversalSerialBusController/>
		<pcic:SerialBusController/>

		<cpu:BranchToRelative8IfNotEqual/>
		<addressOf ref="slotEmpty" type="Relative8"/>

		<cpu:PushSIToStack/>

		<cpu:IncrementBX/>

		<label id="slotEmpty"/>

		<cpu:IncrementSI/>

		<cpu:LoopToRelative8/>
		<addressOf ref="forEachSlot" type="Relative8"/>

		<!--Create List-->
		<cpu:CopyRegisterToOperand/>
		<op:BX-AXRegister/>

		<lst:Create/>

		<cpu:CompareAXToImmediate/>
		<int>0</int>

		<cpu:BranchToRelative8IfEqual/>
		<addressOf ref="createListObject" type="Relative8"/>

		<!--Find PciDevice Classes-->
		<cpu:PushDIToStack/>

		<sys:GetCatalog/>

		<cpu:CopyImmediateToDI/>
		<addressOf ref="ehciUsbController" type="Absolute32"/>

		<cat:FindType/>

		<cpu:CopyRegisterToOperand/>
		<op:BX-CXRegister/>

		<cpu:CopyRegisterToOperand/>
		<op:SI-BXRegister/>

		<cpu:PullDIFromStack/>

		<!--Create PciDevice Object-->
		<label id="forEachDevice"/>

		<cpu:PushDIToStack/>

		<mm:CheckOut length="4"/>

		<cpu:PullSIFromStack/>

		<cpu:PullAXFromStack/>

		<cpu:CopyRegisterToOperand/>
		<op:AX-DIAddress/>

		<cpu:PushSIToStack/>

		<cpu:CopyImmediateToAX/>
		<int>4</int>

		<obj:Create/>

		<cpu:PullSIFromStack/>

		<cpu:PushSIToStack/>

		<cpu:CopyRegisterToOperand/>
		<op:CX-AXRegister/>

		<cpu:DecrementAX/>

		<lst:SetItem/>

		<cpu:PullDIFromStack/>

		<cpu:LoopToRelative8/>
		<addressOf ref="forEachDevice" type="Relative8"/>

		<!--Create List Object-->
		<label id="createListObject"/>

		<!--Get Item Count-->
		<cpu:CopyOperandToRegister/>
		<op:AX-DIAddress/>

		<cpu:IncrementAX/>

		<cpu:ShiftRegisterByImmediate8Function/>
		<shift:ShiftAXRegisterLeft/>
		<byte>2</byte>

		<cpu:PushAXToStack/>
		<cpu:PushSIToStack/>
		<cpu:PushDIToStack/>

		<cpu:CopyImmediateToDI/>
		<addressOf ref="ehciUsbControllerList" type="Absolute32"/>

		<sys:GetCatalog/>

		<cat:FindType/>

		<cpu:CopyRegisterToOperand/>
		<op:SI-BXRegister/>

		<cpu:PullDIFromStack/>
		<cpu:PullSIFromStack/>
		<cpu:PullAXFromStack/>

		<obj:Create/>

		<cpu:ReturnToNearCaller/>
	</cls:method>
	
