		<cpu:PullAXFromStack/>

		<cpu:PushDIToStack/>

		<cpu:CopyAXToDIAddressAndIncrementDI/>

		<cpu:PullDIFromStack/>

		<cpu:CopyImmediateToAX/>
		<int>4</int>

		<obj:Create/>

		<cpu:CopyRegisterToOperand/>
		<op:CX-AXRegister/>

		<cpu:DecrementAX/>

		<cpu:PushSIToStack/>
		
