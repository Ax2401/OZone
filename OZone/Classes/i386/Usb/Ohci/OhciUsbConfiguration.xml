		<cpu:PushSIToStack/>

		<cpu:CopyImmediateToDI/>
		<addressOf ref="list" type="Absolute32"/>

		<sys:GetCatalog/>

		<cat:FindType/>

		<cpu:CopyRegisterToOperand/>
		<op:SI-BXRegister/>

		<cpu:PullDIFromStack/>
		<cpu:PullAXFromStack/>

		<obj:Create/>

		<cpu:PullAXFromStack/>

		<cpu:ReturnToNearCaller/>

		<var:string id="usbDescriptor">http://metalx.org/Usb/UsbDescriptor</var:string>
		<var:string id="list">http://metalx.org/List</var:string>
