		<cpu:SubtractRegisterFromOperand/>
		<op:AX-CXRegister/>
		
		<cpu:BranchToRelative8IfCXIsZero/>
		<addressOf ref="createList" type="Relative8"/>

		<cpu:JumpToRelative8/>
		<addressOf ref="nextDescriptor" type="Relative8"/>

		<label id="createList"/>
		
		<!--Create List-->
		<cpu:CopyRegisterToOperand/>
		<op:BX-AXRegister/>

		<lst:Create/>

		<!--Find Class-->
		<cpu:PushDIToStack/>

		<sys:GetCatalog/>

		<cpu:CopyImmediateToDI/>
