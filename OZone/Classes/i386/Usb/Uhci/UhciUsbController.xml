	<cls:method name="Reset" type="http://metalx.org/Usb/Uhci/UhciUsbController">
		<cpu:CopyRegisterToOperand/>
		<op:DI-SIRegister/>

		<obj:GetData/>

		<cpu:CopyOperandToRegister/>
		<op:AX-SIAddress/>

		<pcif:GetBaseAddress4/>

		<cpu:AndALWithImmediate/>
		<binary>11111100</binary>

		<cpu:CopyRegisterToOperand/>
		<op:AX-DXRegister/>

		<cpu:CopyImmediateToAX/>
		<hex>00000002</hex>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<cpu:ReturnToNearCaller/>
	</cls:method>

	<cls:method name="Enable" type="http://metalx.org/Usb/Uhci/UhciUsbController">
		<cpu:PushDIToStack/>
		
		<cpu:CopyRegisterToOperand/>
		<op:DI-SIRegister/>

		<obj:GetData/>

		<cpu:CopyOperandToRegister/>
		<op:AX-SIAddress/>

		<pcif:GetBaseAddress4/>

		<cpu:AndALWithImmediate/>
		<binary>11111100</binary>

		<cpu:CopyRegisterToOperand/>
		<op:AX-DXRegister/>
		
		<!--Setup Descriptor List-->
		<cpu:MathImmediate8ToOperandFunction/>
		<math:AddToDXRegister/>
		<usb:FrameListAddress/>

		<cpu:CopyImmediateToAX/>
		<hex>00800000</hex>

		<cpu:WriteAXToDXPort/>

		<cpu:MathImmediate8ToOperandFunction/>
		<math:SubtractFromDXRegister/>
		<usb:FrameListAddress/>

		<cpu:CopyImmediateToCX/>
		<int>1024</int>

		<cpu:CopyImmediateToAX/>
		<hex>00810000</hex>

		<cpu:CopyImmediateToDI/>
		<hex>00800000</hex>

		<cpu:Repeat/>
		<cpu:CopyAXToDIAddressAndIncrementDI/>

		<cpu:CopyImmediateToOperandFunction/>
		<imm:CopyImmediateToImmediateAddress/>
		<hex>00810000</hex>
		<hex>00810000</hex>

		<cpu:CopyImmediateToOperandFunction/>
		<imm:CopyImmediateToImmediateAddress/>
		<hex>00810004</hex>
		<hex>00000000</hex>

		<cpu:CopyImmediateToOperandFunction/>
		<imm:CopyImmediateToImmediateAddress/>
		<hex>00810008</hex>
		<hex>00000000</hex>

		<cpu:CopyImmediateToOperandFunction/>
		<imm:CopyImmediateToImmediateAddress/>
		<hex>0081000c</hex>
		<hex>00000000</hex>

		<cpu:CopyImmediateToAX/>
		<hex>00000081</hex>

		<cpu:Operand16/>
		<cpu:WriteAXToDXPort/>

		<cpu:PullDIFromStack/>

		<cpu:ReturnToNearCaller/>
	</cls:method>

