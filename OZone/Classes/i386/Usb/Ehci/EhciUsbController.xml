	<cls:method name="Reset" type="http://metalx.org/Usb/Ehci/EhciUsbController">
		<cpu:CopyRegisterToOperand/>
		<op:DI-SIRegister/>

		<obj:GetData/>

		<cpu:CopyOperandToRegister/>
		<op:AX-SIAddress/>

		<pcif:GetBaseAddress0/>

		<cpu:AndAXWithImmediate/>
		<hex>fffffff0</hex>

		<cpu:CopyRegisterToOperand/>
		<op:AX-SIRegister/>

		<cpu:CopyImmediateToAX/>
		<int>0</int>

		<cpu:CopyOperandToRegister8/>
		<op:AL-SIAddress/>

		<cpu:AddRegisterToOperand/>
		<op:AX-SIRegister/>

		<!--Reset Controller-->
		<cpu:CopyImmediateToAX/>
		<hex>00000002</hex>

		<cpu:CopyRegisterToOperand/>
		<op:AX-SIAddress/>

		<cpu:ReturnToNearCaller/>
	</cls:method>
	
	<cls:method name="Enable" type="http://metalx.org/Usb/Ehci/EhciUsbController">
		<cpu:CopyRegisterToOperand/>
		<op:DI-SIRegister/>

		<obj:GetData/>

		<cpu:CopyOperandToRegister/>
		<op:AX-SIAddress/>

		<pcif:GetBaseAddress0/>

		<cpu:AndAXWithImmediate/>
		<hex>fffffff0</hex>

		<cpu:CopyRegisterToOperand/>
		<op:AX-SIRegister/>

		<cpu:CopyImmediateToAX/>
		<int>0</int>

		<cpu:CopyOperandToRegister8/>
		<op:AL-SIAddress/>

		<cpu:AddRegisterToOperand/>
		<op:AX-SIRegister/>
		
		<!--Setup Periodic Schedule-->
		<!--<cpu:PushDIToStack/>
		
		<cpu:CopyImmediateToCX/>
		<int>1024</int>

		<cpu:CopyImmediateToAX/>
		<hex>00000001</hex>

		<cpu:CopyImmediateToDI/>
		<hex>00950000</hex>

		<cpu:Repeat/>
		<cpu:CopyAXToDIAddressAndIncrementDI/>

		<cpu:PullDIFromStack/>

		<cpu:CopyImmediateToOperandFunction/>
		<imm:CopyImmediateToSIAddressPlusImmediate8/>
		<hex>14</hex>
		<hex>00950000</hex>-->
		
		<!--Enable Interrupts-->
		<!--<cpu:CopyImmediateToAX/>
		<hex>00000028</hex>

		<cpu:CopyRegisterToOperand/>
		<op:AX-SIAddressPlusImmediate8/>
		<hex>08</hex>-->

		<!--Enable Controller-->
		<!--<cpu:CopyOperandToRegister/>
		<op:AX-SIAddress/>

		<cpu:OrAXWithImmediate/>
		<hex>00000001</hex>-->

		<cpu:CopyImmediateToAX/>
		<hex>00080001</hex>

		<cpu:CopyRegisterToOperand/>
		<op:AX-SIAddress/>
		
		<!--Set Configuration-->
		<cpu:CopyImmediateToAX/>
		<int>1</int>

		<cpu:CopyRegisterToOperand/>
		<op:AX-SIAddressPlusImmediate8/>
		<hex>40</hex>

		<cpu:ReturnToNearCaller/>
	</cls:method>

