		<int>283</int>
		<byte>1</byte>

		<cpu:CopyImmediateToOperandFunction8/>
		<imm:CopyImmediateToDIAddressPlusImmediate/>
		<int>284</int>
		<byte>1</byte>

		<!--DHCP Fields Requested (Router, Subnet Mask, DNS Server, Domain Name)-->
		<cpu:CopyImmediateToOperandFunction8/>
		<imm:CopyImmediateToDIAddressPlusImmediate/>
		<int>285</int>
		<byte>55</byte>

		<cpu:CopyImmediateToOperandFunction8/>
		<imm:CopyImmediateToDIAddressPlusImmediate/>
		<int>286</int>
		<byte>4</byte>

		<cpu:CopyImmediateToOperandFunction8/>
		<imm:CopyImmediateToDIAddressPlusImmediate/>
		<int>287</int>
		<byte>3</byte>

		<cpu:CopyImmediateToOperandFunction8/>
		<imm:CopyImmediateToDIAddressPlusImmediate/>
		<int>288</int>
		<byte>1</byte>

		<cpu:CopyImmediateToOperandFunction8/>
		<imm:CopyImmediateToDIAddressPlusImmediate/>
		<int>289</int>
		<byte>6</byte>

		<cpu:CopyImmediateToOperandFunction8/>
		<imm:CopyImmediateToDIAddressPlusImmediate/>
		<int>290</int>
		<byte>15</byte>

		<!--End Of Options-->
		<cpu:CopyImmediateToOperandFunction8/>
		<imm:CopyImmediateToDIAddressPlusImmediate/>
		<int>291</int>
		<byte>255</byte>

		<cpu:CopyRegisterToOperand/>
		<op:DI-SIRegister/>

		<cpu:PushDIToStack/>

		<net:CalculateIpChecksum/>

		<cpu:PullDIFromStack/>

		<cpu:ExchangeOperandWithRegister8/>
		<op:AL-AHRegister/>

		<cpu:Operand16/>
		<cpu:CopyRegisterToOperand/>
		<op:AX-DIAddressPlusImmediate8/>
		<byte>24</byte>

		<obj:Create length="292"/>

		<cpu:ReturnToNearCaller/>
	</cls:method>

