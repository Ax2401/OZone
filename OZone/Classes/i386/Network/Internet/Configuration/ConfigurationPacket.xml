		<imm:CopyImmediateToDIAddressPlusImmediate8/>
		<byte>36</byte>
		<hex>00</hex>

		<cpu:CopyImmediateToOperandFunction8/>
		<imm:CopyImmediateToDIAddressPlusImmediate8/>
		<byte>37</byte>
		<byte>67</byte>

		<!--Length (252)-->
		<cpu:CopyImmediateToOperandFunction8/>
		<imm:CopyImmediateToDIAddressPlusImmediate8/>
		<byte>38</byte>
		<hex>00</hex>

		<cpu:CopyImmediateToOperandFunction8/>
