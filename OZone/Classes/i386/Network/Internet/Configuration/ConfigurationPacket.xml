		<cpu:CopyImmediateToOperandFunction8/>
		<imm:CopyImmediateToDIAddressPlusImmediate8/>
		<byte>53</byte>
		<hex>00</hex>

		<!--Client IP Address-->
		<cpu:CopyImmediateToOperandFunction/>
		<imm:CopyImmediateToDIAddressPlusImmediate8/>
		<byte>54</byte>
		<hex>00000000</hex>

		<!--Client IP Address-->
		<cpu:CopyImmediateToOperandFunction/>
		<imm:CopyImmediateToDIAddressPlusImmediate8/>
		<byte>58</byte>
		<hex>00000000</hex>

		<!--Client IP Address-->
		<cpu:CopyImmediateToOperandFunction/>
		<imm:CopyImmediateToDIAddressPlusImmediate8/>
		<byte>62</byte>
		<hex>00000000</hex>

		<!--Client IP Address-->
		<cpu:CopyImmediateToOperandFunction/>
		<imm:CopyImmediateToDIAddressPlusImmediate8/>
		<byte>66</byte>
		<hex>00000000</hex>

