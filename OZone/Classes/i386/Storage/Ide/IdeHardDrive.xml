		<cpu:CopyOperandToRegister/>
		<op:DX-SIAddress/>

		<cpu:CopyOperandToRegister8/>
		<op:AL-SIAddressPlusImmediate8/>
		<byte>4</byte>

		<hd:GetSerialNumber/>

		<str:CreateObject/>

		<cpu:ReturnToNearCaller/>
	</cls:method>

