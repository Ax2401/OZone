		<cpu:CopyOperandToRegister/>
		<op:SI-SIAddress/>

		<cpu:CopyRegisterToOperand/>
		<op:SI-ImmediateAddress/>
		<addressOf ref="reader" type="Absolute32"/>

		<!--Load Folder Record-->
		<cpu:MathImmediate8ToOperandFunction/>
