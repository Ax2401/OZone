	<cls:field name="Device" type="http://metalx.org/Pci/PciDevice" offset="0"/>
	
	<cls:method name="Test" type="http://metalx.org/Integer" static="true">
		<!--Feedback[0]: None-->
		<cpu:CopyImmediateToDX/>
		<hex>00000388</hex>

		<cpu:CopyImmediateToAL/>
		<hex>c0</hex>
		
		<cpu:WriteALToDXPort/>

		<cpu:IncrementDX/>

		<cpu:CopyImmediateToAL/>
		<hex>01</hex>

		<cpu:WriteALToDXPort/>

		<cpu:DecrementDX/>
		
		<!--Modulation[0]: Normal-->
		<!--<cpu:CopyImmediateToAL/>
		<hex>20</hex>
		
		<cpu:WriteALToDXPort/>

		<cpu:IncrementDX/>

		<cpu:CopyImmediateToAL/>
		<binary>00100001</binary>

		<cpu:WriteALToDXPort/>

		<cpu:DecrementDX/>-->

		<!--Scaling[0]: 0dB Attenuation-->
		<!--<cpu:CopyImmediateToAL/>
		<hex>40</hex>

		<cpu:WriteALToDXPort/>

		<cpu:IncrementDX/>

		<cpu:CopyImmediateToAL/>
		<hex>00</hex>

		<cpu:WriteALToDXPort/>

		<cpu:DecrementDX/>-->

		<!--Attack[0]: 0xf Decay[0]: 0x0-->
		<!--<cpu:CopyImmediateToAL/>
		<hex>60</hex>

		<cpu:WriteALToDXPort/>

		<cpu:IncrementDX/>

		<cpu:CopyImmediateToAL/>
		<hex>f0</hex>

		<cpu:WriteALToDXPort/>

		<cpu:DecrementDX/>-->

		<!--Sustain[0]: 0x0 Release[0]: 0xf-->
		<!--<cpu:CopyImmediateToAL/>
		<hex>80</hex>

		<cpu:WriteALToDXPort/>

		<cpu:IncrementDX/>

		<cpu:CopyImmediateToAL/>
		<hex>0f</hex>

		<cpu:WriteALToDXPort/>

		<cpu:DecrementDX/>-->

		<!--Modulation[3]: Normal Sustain-->
		<cpu:CopyImmediateToAL/>
		<hex>23</hex>

		<cpu:WriteALToDXPort/>

		<cpu:IncrementDX/>

		<cpu:CopyImmediateToAL/>
		<binary>00100001</binary>

		<cpu:WriteALToDXPort/>

		<cpu:DecrementDX/>

		<!--Scaling[3]: 0dB Attenuation-->
		<cpu:CopyImmediateToAL/>
		<hex>43</hex>

		<cpu:WriteALToDXPort/>

		<cpu:IncrementDX/>

		<cpu:CopyImmediateToAL/>
		<hex>00</hex>

		<cpu:WriteALToDXPort/>

		<cpu:DecrementDX/>

		<!--Attack[3]: 0xf Decay[3]: 0x0-->
		<cpu:CopyImmediateToAL/>
		<hex>63</hex>

		<cpu:WriteALToDXPort/>

		<cpu:IncrementDX/>

		<cpu:CopyImmediateToAL/>
		<hex>f0</hex>

		<cpu:WriteALToDXPort/>

		<cpu:DecrementDX/>

		<!--Sustain[3]: 0x0 Release[3]: 0xf-->
		<cpu:CopyImmediateToAL/>
		<hex>83</hex>

		<cpu:WriteALToDXPort/>

		<cpu:IncrementDX/>

		<cpu:CopyImmediateToAL/>
		<hex>0f</hex>

		<cpu:WriteALToDXPort/>

		<cpu:DecrementDX/>
		
		<!--Get Frequency-->
		<cpu:CopyImmediateToAX/>
		<int>45</int>

		<cpu:Operand16/>
		<cpu:CopyOperandToRegister/>
		<op:BX-IndexAddress/>
		<index:AXTimesTwoPlusImmediate32/>
		<addressOf ref="notes" type="Absolute32"/>

		<!--F-Number[0]: 0x41-->
		<cpu:CopyImmediateToAL/>
		<hex>a0</hex>

		<cpu:WriteALToDXPort/>

		<cpu:IncrementDX/>

		<!--<cpu:CopyImmediateToAL/>
		<hex>41</hex>-->
		
		<cpu:CopyRegisterToOperand8/>
		<op:BL-ALRegister/>

		<cpu:WriteALToDXPort/>

		<cpu:DecrementDX/>

		<!--KeyOn[0]: F-Number High: 2 Octave: 4 Key: On-->
		<cpu:CopyImmediateToAL/>
		<hex>b0</hex>

		<cpu:WriteALToDXPort/>

		<cpu:IncrementDX/>

		<!--<cpu:CopyImmediateToAL/>
		<hex>32</hex>-->
		
		<cpu:CopyRegisterToOperand8/>
		<op:BH-ALRegister/>
		
		<cpu:OrALWithImmediate/>
		<binary>00100000</binary>

		<cpu:WriteALToDXPort/>

		<cpu:CopyImmediateToDI/>
		<int>0</int>
	
		<cpu:ReturnToNearCaller/>
	</cls:method>

	<cls:method name="ToString" type="http://metalx.org/String">
		<cpu:CopyImmediateToDI/>
		<addressOf ref="deviceName" type="Absolute32"/>

		<str:CreateObject/>

		<cpu:ReturnToNearCaller/>

		<var:string id="deviceName">Yamaha OPL3 Synthesizer</var:string>
	</cls:method>

	<cls:method name="GetClass" type="http://metalx.org/Class">
		<cpu:CopyRegisterToOperand/>
		<op:DI-SIRegister/>

		<obj:GetType/>

		<cpu:CopyRegisterToOperand/>
		<op:SI-DIRegister/>

		<clsf:CreateObject/>

		<cpu:ReturnToNearCaller/>
	</cls:method>

	<cls:method name="GetClass" type="http://metalx.org/Class" static="true">
		<cpu:CopyRegisterToOperand/>
		<op:BX-DIRegister/>

		<clsf:CreateObject/>

		<cpu:ReturnToNearCaller/>
	</cls:method>

