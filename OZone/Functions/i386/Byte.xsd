<?xml version="1.0" encoding="utf-8"?>
<xsd:schema
    targetNamespace="http://metalx.org/i386/Functions/Byte"
    elementFormDefault="qualified"
    xmlns="http://metalx.org/i386/Functions/Byte"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema">

	<xsd:element name="CreateObject">
		<xsd:annotation>
			<xsd:documentation>
				Creates a new Byte object.

				Input:
				AL=Value

				Output:
				DI=Object
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType/>
	</xsd:element>

	<xsd:element name="Parse">
		<xsd:annotation>
			<xsd:documentation>
				Converts the specified string into a Byte.

				Input:
				SI=String

				Output:
				AL=Value
				
				Modifies: AX BX CX DX SI DI
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType/>
	</xsd:element>

	<xsd:element name="ToString">
		<xsd:annotation>
			<xsd:documentation>
				Converts a byte value to a string.

				Input:
				AL=Value

				Output:
				DI=String

				Modifies: AX BX CX DX
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:attribute name="digits" type="xsd:byte"/>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
