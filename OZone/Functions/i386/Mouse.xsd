<?xml version="1.0" encoding="utf-8"?>
<xsd:schema
    targetNamespace="http://metalx.org/i386/Functions/Mouse"
    elementFormDefault="qualified"
    xmlns="http://metalx.org/i386/Functions/Mouse"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema">

	<xsd:element name="Enable">
		<xsd:annotation>
			<xsd:documentation>
				Enables the PS/2 mouse.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType/>
	</xsd:element>

	<xsd:element name="Disable">
		<xsd:annotation>
			<xsd:documentation>
				Disables the PS/2 mouse.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType/>
	</xsd:element>

</xsd:schema>
